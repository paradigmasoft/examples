<?xml version="1.0" encoding="utf-8" ?>
<examples>
	<databases>
		<database type="vdb" name="valentina_sakila_using_foreign_key">
			<tooltips>
				<tooltip language="en" value="Sakila database ported to Valentina DB model, uses Foreign Key links between tables, like other relational databases"/>
			</tooltips>
			<versions>
				<version min_version="5.0" link="./vdb/valentina_sakila_using_foreign_key.vdb.zip"/>
			</versions>
		</database>
		<database type="vdb" name="valentina_sakila_using_binarylink">
			<tooltips>
				<tooltip language="en" value="Sakila database ported to Valentina DB model, uses Binary Links between tables"/>
			</tooltips>	
			<versions>
				<version min_version="5.0" link="./vdb/valentina_sakila_using_binarylink.vdb.zip"/>
			</versions>
		</database>
		<database type="vdb" name="valentina_sakila_using_objectptr">
			<tooltips>
				<tooltip language="en" value="Sakila database ported to Valentina DB model, uses ObjectPtr links between tables"/>
			</tooltips>	
			<versions>
				<version min_version="5.0" link="./vdb/valentina_sakila_using_objectptr.vdb.zip"/>
			</versions>
		</database>

		<database type="sqlite" name="sqlite_sakila">
			<tooltips>
				<tooltip language="en" value="Sakila database ported to SQLite DB model"/>
			</tooltips>	
			<versions>
				<version min_version="5.0" link="./sqlite/sqlite_sakila.sqlite.zip"/>
			</versions>
		</database>
        
        <database type="vdb" name="car_catalog_db" optional="true">
            <tooltips>
                <tooltip language="en" value="Database with source data for car_catalog example project"/>
            </tooltips>
            <versions>
                <version min_version="10.5" link="./vdb/car_catalog_db.vdb.zip"/>
            </versions>
        </database>

	</databases>
	<projects>
		<project name="valentina_sakila_reports" datasource="valentina_sakila_using_foreign_key" datasource_type="vdb">
			<tooltips>
				<tooltip language="en" value="Valentina Reports created on the basis of Sakila database ported to Valentina DB model"/>
			</tooltips>
			<versions>
				<version  min_version="5.0" link = "./vsp/valentina_sakila_reports.vsp.zip"/>
			</versions>
		</project>
        
        <project name="car_catalog_js" datasource="car_catalog_db" datasource_type="vdb">
            <tooltips>
                <tooltip language="en" value="Valentina Reports and Valentina Forms to fill and print car catalogs using JavaScript"/>
            </tooltips>
            <versions>
                <version  min_version="14" link = "./vsp/car_catalog_js.vsp.zip"/>
            </versions>
        </project>
        
        <project name="car_catalog_python" datasource="car_catalog_db" datasource_type="vdb">
            <tooltips>
                <tooltip language="en" value="Valentina Reports and Valentina Forms to fill and print car catalogs using Python"/>
            </tooltips>
            <versions>
                <version  min_version="14" link = "./vsp/car_catalog_python.vsp.zip"/>
            </versions>
        </project>
        
         <project name="schema_report" datasource="valentina_sakila_using_foreign_key" datasource_type="vdb">
            <tooltips>
                <tooltip language="en" value="Valentina Reports and Valentina Forms to print schema documentation"/>
            </tooltips>
            <versions>
                <version  min_version="13.5" link = "./vsp/schema_report.vsp.zip"/>
            </versions>
        </project>
	</projects>
</examples>
